int x,y;

void setup(){
 size(500,500);
x = 50;
y= 50;
}

void draw(){
  background(0);
  ellipse(250,250,x,y);
  if(keyPressed && key == CODED) {
    if(keyCode == LEFT){
      x = x-1;
    }
  else if(keyCode == RIGHT){
      x = x+1;
  }
  else if(keyCode == UP){
    y = y-1;
  }
  else if(keyCode == DOWN){
    y = y+1;
  }
  }
}
